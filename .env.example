# Email (Optional)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASS=your-smtp-password
FROM_EMAIL=noreply@psicoapp.com

# Stream
STREAM_API_KEY=your-stream-api-key
STREAM_API_SECRET=your-stream-api-secret 